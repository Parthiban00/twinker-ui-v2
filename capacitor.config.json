{
  "appId": "com.parthitechs.starter",
  "appName": "Twinker",
  "webDir": "www/browser",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0,
      "launchAutoHide": true,
      "showSpinner": false
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  },
  "server": {
    "allowNavigation": [
      "maps.googleapis.com",
      "maps.google.com",
      "*.googleapis.com",
      "*.gstatic.com",
      "*.google.com"
    ]
  }
}
